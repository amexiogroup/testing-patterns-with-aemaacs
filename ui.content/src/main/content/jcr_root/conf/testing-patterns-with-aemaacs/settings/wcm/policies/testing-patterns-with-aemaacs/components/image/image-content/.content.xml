<?xml version='1.0' encoding='UTF-8'?>
<jcr:root  xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          enableDmFeatures="true" allowedRenditionWidths="[320,480,600,800,1024,1200,1600]"
          disableLazyLoading="false" enableAssetDelivery="true" allowUpload="false"
          altValueFromDAM="true" displayPopupTitle="true" isDecorative="false" jpegQuality="{Long}85"
          titleValueFromDAM="true" uuidDisabled="true"
          jcr:description="- Enable lazy loading for faster page loading.&#13;&#10;- Define allowed image widths for automatic loading of the image in the most optimal resolution, given the size at which each image is displayed on the page, and the visitor's pixel density.&#13;&#10;- Disallow direct asset upload, to force authors to first upload the images to Assets."
          jcr:primaryType="nt:unstructured" jcr:title="Content Image"
          sling:resourceType="wcm/core/components/policy/policy">
    <jcr:content jcr:primaryType="nt:unstructured"
                 cq:lastReplicated="{Date}2030-01-01T00:00:00.000Z"
                 cq:lastReplicationAction="Activate"/>
    <plugins jcr:primaryType="nt:unstructured">
        <crop features="*" jcr:primaryType="nt:unstructured">
            <aspectRatios jcr:primaryType="nt:unstructured">
                <item0 name="Wide Landscape" ratio="0.6180" jcr:primaryType="nt:unstructured"/>
                <item1 name="Landscape" ratio="0.8284" jcr:primaryType="nt:unstructured"/>
                <item2 name="Square" ratio="1" jcr:primaryType="nt:unstructured"/>
                <item3 name="Portrait" ratio="1.6180" jcr:primaryType="nt:unstructured"/>
            </aspectRatios>
        </crop>
        <rotate features="right" jcr:primaryType="nt:unstructured"/>
        <flip features="-" jcr:primaryType="nt:unstructured"/>
    </plugins>
</jcr:root>
